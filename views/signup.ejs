<%-  include ('../partials/header') %>
<link rel="stylesheet" href="/css/signup.css">  
</head>
<%-  include ('../partials/nav') %>

<form>
      <div class="form-group">
          <label for="firstname">Firstname</label>
          <input type="text" class="form-control" placeholder="Miracle">
      </div>
      <div class="form-group">
          <label for="lastname">Lastname</label>
          <input type="text" class="form-control" placeholder="Billy">
      </div>

    <div class="form-group">
          <label for="username">Username</label>
          <input type="text" class="form-control" placeholder="mimi">
      </div>
      
      <div class="form-group">
          <label for="email">Email address</label>
          <input type="email" class="form-control" placeholder="miraclebilly@gmail.com">
          <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>

    <div class="form-group">
      <label for="password">Password</label>
      <input type="password" class="form-control" placeholder="*********">
    </div>
    
    
    <div class="form-group">
      <label for="repeatpassword">Repeat Password</label>
      <input type="password" class="form-control" placeholder="********">
    </div>
   
    <button type="submit" class="btn btn-primary">Sign Up</button>
  </form>

    <%-  include ('../partials/footer') %>